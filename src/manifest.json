{
	"name": "__MSG_extName__",
	"version": "2.0.2",
	"short_name": "Bookmarks",
	"manifest_version": 2,
	"description": "__MSG_extDesc__",
	"icons": {
		"16": "images/icon16@2x.png",
		"32": "images/icon32@2x.png",
		"48": "images/icon48@2x.png",
		"128": "images/icon128@2x.png"
	},
	"browser_action": {
		"default_icon": "images/star-dark-128.png",
		"default_popup": "popup.html",
		"default_title": "__MSG_extActionTitle"
	},
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	},
	"background": {
		"scripts": ["background.js"]
	}, 
	"permissions": [
		"http://*/*",
		"https://*/*",
		"chrome://favicon/",
		"bookmarks",
		"tabs",
		"storage",
		"contextMenus"
	],
	"minimum_chrome_version": "63",
	"default_locale": "en"
}
